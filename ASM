	AREA PROG1, CODE, READONLY
	ENTRY
START
	LDR R0, =COUNT
	LDR R1, [R0]
	LDR R2, =ARRAY
	LDR R3,[R2],#4
	B BACK
BACK SUBS R1,R1,#01
	BEQ FWD
	LDR R4
	CMP R3,R4
	BGT BACK
	MOV R3,R4
	B BACK
FWD LDR R5,=RESULT
	STR R3,[R5]
EXIT B EXIT
COUNT DCD 0X07
ARRAY
	DCD 0X15
	DCD 0X35
	DCD 0X32
	DCD 0X45
	DCD 0X10
	DCD 0X4F
	DCD 0X34
	AREA DATA2,DATA,READWRITE
RESULT DCD 0X0
	END
